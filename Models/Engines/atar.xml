<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>atar.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>atar</object-name>
    <object-name>helice1</object-name>
    <object-name>helice1</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>spin</type>
    <object-name>helice1</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <factor> 100 </factor>
    <center>
      <x-m> -1.560 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.000 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>helice2</object-name>
    <property>controls/engines/engine[1]/throttle</property>
    <factor> 100 </factor>
    <center>
      <x-m>  1.487 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.000 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>

